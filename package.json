{
  "name": "svelte-webflow-cms",
  "version": "0.0.40",
  "description": "A config-driven CMS table editor for managing Webflow collections with SvelteKit",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./providers/r2": {
      "types": "./dist/server/providers/r2-upload-provider.d.ts",
      "default": "./dist/server/providers/r2-upload-provider.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package -o dist",
    "prepublishOnly": "bun run build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "peerDependencies": {
    "svelte": "^5.0.0",
    "@sveltejs/kit": "^2.0.0",
    "bits-ui": "^2.0.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.48.5",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^24",
    "bits-ui": "^2.14.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.43.8",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.561.0",
    "browser-image-compression": "^2.0.2",
    "clsx": "^2.1.1",
    "svelte-dnd-action": "^0.9.68",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "webflow-api": "^3.2.1"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "webflow",
    "cms",
    "table",
    "editor"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/briantuckerdesign/svelte-webflow-cms"
  }
}
